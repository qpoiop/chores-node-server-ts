{
    "name": "em-chores-node",
    "version": "0.9.0",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "start:dev": "cross-env NODE_ENV=development nodemon",
        "start:pm2": "pm2 start processes.js --no-autorestart",
        "stop:pm2": "pm2 stop all",
        "build:babel": "babel src --extensions .ts -d dist --ignore **/__tests__/**",
        "build:tsc": "tsc -p tsconfig.json && tsc-alias",
        "lint": "eslint 'src/**/*.js'",
        "generate": "prisma generate",
        "introspection": "prisma db pull --schema=./src/prisma/schema.prisma",
        "seed": "ts-node ./src/prisma/seeds",
        "migrate": "prisma migrate dev --preview-feature --schema=./src/prisma/schema.prisma",
        "test:jest": "jest --coverage",
        "e2e:setup": "cd e2e && yarn install",
        "e2e:start": "cd e2e && yarn test",
        "e2e:meas": "cd e2e && yarn perf"
    },
    "dependencies": {
        "@prisma/client": "^4.10.1",
        "bcrypt": "^5.1.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-rate-limit": "^6.7.0",
        "express-session": "^1.17.3",
        "express-ws": "^5.0.2",
        "helmet": "^6.0.1",
        "jsonwebtoken": "^9.0.0",
        "lodash": "^4.17.20",
        "lusca": "^1.7.0",
        "morgan": "^1.10.0",
        "multer": "^2.0.0-rc.4",
        "mysql": "^2.18.1",
        "passport": "^0.6.0",
        "passport-jwt": "^4.0.1",
        "pm2": "5.2.2",
        "winston": "^3.8.2",
        "ws": "8.12.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@babel/eslint-parser": "^7.19.1",
        "@babel/node": "^7.20.7",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@babel/plugin-proposal-optional-chaining": "^7.20.7",
        "@babel/plugin-transform-object-assign": "^7.18.6",
        "@babel/preset-env": "^7.20.0",
        "@babel/preset-typescript": "^7.18.6",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.17",
        "@types/express-serve-static-core": "^4.17.33",
        "@types/express-ws": "^3.0.1",
        "@types/jest": "^29.4.0",
        "@types/node": "^18.13.0",
        "@types/ws": "^8.5.4",
        "@typescript-eslint/eslint-plugin": "^5.52.0",
        "babel-plugin-lodash": "^3.3.4",
        "babel-plugin-root-import": "^5.1.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.33.0",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-config-standard-with-typescript": "^34.0.0",
        "eslint-import-resolver-babel-plugin-root-import": "1.1.1",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "faker": "4.1.0",
        "jest": "29.4.2",
        "nodemon": "2.0.20",
        "prettier": "2.8.4",
        "prettier-plugin-prisma": "^4.10.0",
        "prisma": "^4.10.1",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.8.2",
        "tsconfig-paths": "^4.1.2",
        "typescript": "^4.9.5",
        "yarn": "^1.22.19"
    }
}
